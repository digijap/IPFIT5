import HTML

# open een HTML bestand waarin de output moet worden gezet

HTMLFILE = 'IPFIT5.html'
f = open(HTMLFILE, 'w')

# Titel
titel = 'IFPIT5 HTML rapport'
f.write(titel)
f.write('<p>')
f.write('<title> Ipfit5 Html rapport </title>')

# genereer case data vanuit sleutel
casenaam = 'test'
casenr = '1' 
onderzoeker = 'Dylan Bragonje'

# printen van de casedata naar het html bestand
table_data = [
        ['Case',   casenaam],
        ['Casenummer', casenr],
        ['Onderzoeker' , onderzoeker]
]
htmlcode = HTML.table(table_data)
print htmlcode
f.write(htmlcode)
f.write('<p>')
print '-'*79

